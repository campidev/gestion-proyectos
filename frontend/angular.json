{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"shell-app": {
			"projectType": "application",
			"schematics": {},
			"root": "projects/shell-app",
			"sourceRoot": "projects/shell-app/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/shell-app",
						"index": "projects/shell-app/src/index.html",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/shell-app/tsconfig.app.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/shell-app/public",
								"output": "."
							},"projects/shell-app/src/assets"
						],
						"styles": [
						"node_modules/bootstrap/dist/css/bootstrap.min.css",
						"@angular/material/prebuilt-themes/azure-blue.css",
							"projects/shell-app/src/styles.css"
						],
						"scripts": [
						"node_modules/jquery/dist/jquery.min.js",
						"node_modules/popper.js/dist/umd/popper.min.js",
						"node_modules/bootstrap/dist/js/bootstrap.min.js"
						],
						"main": "projects/shell-app/src/main.ts",
						"extraWebpackConfig": "projects/shell-app/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kB",
									"maximumError": "1MB"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kB",
									"maximumError": "4kB"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/shell-app/webpack.prod.config.js"
						},
						"development": {
							"optimization": false,
							"extractLicenses": false,
							"sourceMap": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"buildTarget": "shell-app:build:production",
							"extraWebpackConfig": "projects/shell-app/webpack.prod.config.js"
						},
						"development": {
							"buildTarget": "shell-app:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"ssl": true,
						"sslKey": "projects/shell-app/ssl/key.pem",
						"sslCert": "projects/shell-app/ssl/cert.pem",
						"port": 4200,
						"publicHost": "https://localhost:4200",
						"extraWebpackConfig": "projects/shell-app/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/shell-app/tsconfig.spec.json",
						"assets": [
							{
								"glob": "**/*",
								"input": "projects/shell-app/public"
							}
						],
						"styles": [
							"projects/shell-app/src/styles.css"
						],
						"scripts": []
					}
				}
			}
		},
